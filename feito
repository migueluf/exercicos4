Void remove (int index) throws EmptyStackException {
	If (empty())
		Throw new EmptyStackException (‘’Lista Vazia’’);
	If (index==0){
		First=first.next;
}else{
		For(int i=0; i< index-1; i++)
			Cursor = cursor.next;
			Cursor.next=cursor.next.next;
}
