A -
 void addFirst (Object v){
	Node t= new Node(v);
	If (head==null){
		t.next=t;
		head=t;
}else{
	t.next= head.next;
	head.next=t;
	head=t;
}
}
B -
Void removeFirst(){
	If(head!=null){
	If (head==head.next)
		Head=null;
else
	head.next= head.next.next;
}
}
