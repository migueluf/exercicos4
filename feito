D –
Void imprime(){
	ListNode cursor = first;

	while (cursor!=null){
		System.out.println(cursor.val);
		cursor= cursor.next;
}
}
Void print (){
	For(ListNode cursor= first; cursor!= null; cursor=cursor.next)
		System.out.println(cursor.val);
}

